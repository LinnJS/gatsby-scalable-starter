{
  "name": "gatsby-scalable-starter",
  "description": "Gatsby Scalable Starter, the starter with all the extras to quick start a scalable GatsbyJS application",
  "version": "0.1.0",
  "author": "Justin Linn <linnjsconsulting@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinnJS/gatsby-scalable-starter"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "styled-components",
    "reset-css",
    "eslint",
    "netlify",
    "scalable"
  ],
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "open": "gatsby develop -H 0.0.0.0 -o",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "build:all": "gatsby build && build-storybook -c .storybook -o storybook",
    "serve": "gatsby serve -o -p 8080",
    "clean": "gatsby clean",
    "lint": "eslint src -c .eslintrc.js --ext js,jsx",
    "lint:fix": "eslint src -c .eslintrc.js --fix --ext js,jsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "dependencies": {
    "gatsby": "^2.22.9",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0",
    "styled-components": "^5.1.0",
    "styled-reset-advanced": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "yarn-run-all": "^3.1.1"
  }
}
